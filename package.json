{
  "name": "nyardpress",
  "version": "1.0.0",
  "description": "WordPress development environment",
  "scripts": {
    "setup": "run-s setup:env dc:install dc:build",
    "rebuild": "run-s dc:destroy dc:install dc:build",
    "start": "run-s dc:start",
    "dump": "run-s dc:dump",
    "restore": "run-s dc:restore:latest",
    "setup:env": "node scripts/setup-env.js",
    "dc:install": "node scripts/dc-install.js",
    "dc:destroy": "node scripts/dc-destory.js",
    "dc:build": "node scripts/dc-build.js",
    "dc:start": "docker compose up -d",
    "dc:dump": "node scripts/dc-dump.js",
    "dc:restore": "node scripts/dc-restore.js",
    "dc:restore:latest": "node scripts/dc-restore.js --latest"
  },
  "private": true,
  "devDependencies": {
    "npm-run-all2": "^8.0.4"
  }
}
